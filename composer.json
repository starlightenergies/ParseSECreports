{
	"name": "james/vaultbear",
	"description": "Nette database, VaultBear Parser to make sense of a treasure trove of financial data.",
	"keywords": ["nette", "database","SEC","financial","stocks"],
	"homepage": "https://www.vaultbear.com",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "James Danforth",
			"homepage": "https://www.reemotex.com"
		}
	],
	"require": {
		"php": ">=7.3.24",
		"ext-pdo": "*",
		"nette/caching": "^3.0",
		"nette/utils": "^3.2.1",
		"nette/http": "v4.0.x-dev",
		"guzzlehttp/guzzle": "7.4.x-dev"
	},
	"require-dev": {
		"nette/tester": "^2.4",
		"nette/di": "^v3.0",
		"mockery/mockery": "^1.3.4",
		"tracy/tracy": "^2.4",
		"phpstan/phpstan-nette": "^0.12"
	},
	"conflict": {
		"nette/di": "<3.0-stable"
	},
	"autoload": {
		"classmap": ["src/","classes/"],
		"psr-4": {
			"JDApp\\": "classes/"
			}
		},

	"minimum-stability": "dev",
	"scripts": {
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.1-dev"
		}
	}
}
